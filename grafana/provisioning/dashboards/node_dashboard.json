{
    "annotations": { "list": [] },
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          { "expr": "100 - (avg by(instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)" }
        ]
      },
      {
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          { "expr": "node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes" }
        ]
      }
    ],
    "schemaVersion": 27,
    "title": "Node Monitoring Dashboard"
  }
  